{
  "name": "@applitools/eyes-browser-extension",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-browser-extension"
  },
  "license": "ISC",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "browser-extension",
    "extension"
  ],
  "scripts": {
    "lint": "eslint '**/*.js'",
    "build": "yarn build:dist && yarn build:zip",
    "build:dist": "webpack",
    "build:zip": "cd dist; zip -r eyes-browser-extension.zip *; mv eyes-browser-extension.zip ../",
    "generate:tests": "coverage-tests generate ./test/coverage/config.js --name 'eyes-browser-extension'",
    "test": "yarn test:it && yarn test:coverage",
    "test:sanity": "yarn test:it",
    "test:it": "mocha --no-timeouts --exit ./test/it/*.spec.js",
    "test:coverage": "yarn generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-browser-extension' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "report": "coverage-tests report https://raw.githubusercontent.com/applitools/sdk.coverage.tests/universal-sdk/js/config.js --name 'eyes-browser-extension'",
    "deps": "bongo deps",
    "gh:test": "gh workflow run test.yml --ref $(git rev-parse --abbrev-ref HEAD) -f packages='browser-extension' -f links='eyes-sdk-core eyes-api types utils driver screenshoter visual-grid-client'",
    "gh:publish": "gh workflow run publish-browser-extension.yml --ref $(git rev-parse --abbrev-ref HEAD)",
    "preversion": "bongo preversion --sviv",
    "version": "yarn build && bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "13.11.11",
    "@applitools/utils": "1.3.13",
    "@applitools/visual-grid-client": "15.14.1",
    "webextension-polyfill": "0.7.0"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.6",
    "@applitools/scripts": "^1.2.0",
    "@applitools/sdk-coverage-tests": "^2.3.15",
    "@applitools/spec-driver-playwright": "^1.3.4",
    "@applitools/test-utils": "^1.5.2",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "canvas-browserify": "^1.1.3",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.18.1",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "https-browserify": "^1.0.0",
    "mocha": "^8.3.0",
    "net-browserify": "^0.2.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "playwright": "^1.22.2",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "selenium-webdriver": "^4.0.0-beta.4",
    "spec-xunit-file": "^0.0.1-3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tls-browserify": "^0.2.2",
    "tty-browserify": "^0.0.1",
    "util": "^0.12.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
