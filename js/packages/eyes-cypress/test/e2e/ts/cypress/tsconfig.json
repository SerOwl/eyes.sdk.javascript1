{
  "include": ["**/*.ts"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "moduleResolution": "node16",
    "lib": ["es6", "dom"],
    "types": ["cypress", "../../../.."],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmit": true,
    "esModuleInterop": true
  }
}
